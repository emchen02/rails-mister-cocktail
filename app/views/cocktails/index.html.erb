<h1>Cocktails Galore</h1>
<div class="container">
<% @cocktails.each do |cocktail| %>
  <%= link_to cocktail_path(cocktail) do %>
  <%= cl_image_tag cocktail.photo.key, width: 200, height: 200, crop: :fill %>
  <!-- link_to String, cocktail.name is a string-->
  <% end %>
